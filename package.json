{
  "name": "quenta",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "node build",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check-updates": "npm-check-updates --cache",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "fmt": "$npm_execpath run format",
    "fmt:check": "$npm_execpath run format:check",
    "tidy": "$npm_execpath run format; $npm_execpath run lint",
    "knip": "knip",
    "graphile-migrate": "env-cmd --silent graphile-migrate --config ./gmrc.cjs"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/compat": "1.2.9",
    "@eslint/js": "9.26.0",
    "@flydotio/dockerfile": "0.7.10",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-svelte-csf": "5.0.0",
    "@storybook/svelte": "8.6.12",
    "@storybook/sveltekit": "8.6.12",
    "@sveltejs/adapter-node": "5.2.12",
    "@sveltejs/kit": "2.20.8",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@types/pg": "8.15.0",
    "env-cmd": "10.1.0",
    "eslint": "9.26.0",
    "eslint-plugin-svelte": "3.5.1",
    "globals": "16.1.0",
    "graphile-migrate": "1.4.1",
    "knip": "5.55.1",
    "npm-check-updates": "18.0.1",
    "storybook": "8.6.12",
    "svelte": "5.28.2",
    "svelte-check": "4.1.7",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.0",
    "vite": "6.3.5",
    "vitest": "3.1.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["@biomejs/biome", "esbuild", "svelte-preprocess"]
  },
  "knip": {
    "svelte": {
      "entry": [
        "./src/routes/**/+{layout,page,server,error}{,.server}.{svelte,ts}",
        "./svelte.config.js"
      ],
      "project": ["./src/**/*.ts"]
    },
    "paths": {
      "$lib/*": ["src/lib/*"],
      "$app/*": ["node_modules/@sveltejs/kit/src/runtime/app/*"],
      "$env/*": ["node_modules/@sveltejs/kit/src/runtime/env/*"]
    },
    "ignore": [".ncurc.js"],
    "ignoreDependencies": ["graphile-migrate"],
    "ignoreExportsUsedInFile": true
  },
  "dependencies": {
    "modern-normalize": "^3.0.1",
    "pg": "8.15.6"
  }
}
